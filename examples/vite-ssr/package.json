    "@vitejs/plugin-vue": "^3.1.0",
    "compression": "^1.7.4",
    "serve-static": "^1.15.0",
    "typescript": "^4.8.4",
    "vite": "^3.1.4",
    "vue-tsc": "^0.40.13",
    "zeed-dom": "^0.9.26",
    "zerva": "^0.5.8"
  }
