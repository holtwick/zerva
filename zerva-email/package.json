    "test": "echo 'NO TESTS AVAILABLE'",
    "watch": "tsup src/index.ts --watch"
  },
  "dependencies": {
    "@zerva/core": "workspace:*",
    "@zerva/http": "workspace:*",
    "nodemailer": "^6.7.7",
    "zeed": "^0.8.6"
  },
  "devDependencies": {
    "@types/node": "^18.6.2",
    "@types/nodemailer": "^6.4.4",
    "tsup": "^6.2.0"
